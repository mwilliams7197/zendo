{
  "name": "Pendo",
  "version": "0.1.1",
  "homepage":".",
  "private": true,
  "devDependencies": {
    "react-scripts": "1.0.7"
  },
  "dependencies": {
    "dateformat": "^2.0.0",
    "es6-store": "^1.1.0",
    "material-ui": "^0.18.6",
    "prop-types": "^15.5.10",
    "ramda": "^0.24.1",
    "react": "^15.5.4",
    "react-dom": "^15.5.4",
    "react-helmet": "4",
    "react-router": "^4.1.2",
    "react-router-dom": "^4.1.1",
    "react-snapshot": "^1.1.0",
    "react-tap-event-plugin": "^2.0.1",
    "recycle": "^2.2.2",
    "rxjs": "^5.4.1",
    "styled-components": "^2.0.0",
    "whatwg-fetch": "^2.0.3"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build && cp -r ./build/* ./zendesk-app/assets",
    "build:prod": "REACT_APP_HOST_ENV=production react-scripts build && cp -r ./build/* ./zendesk-app/assets",
    "clean": "rm ./zendesk-app/assets/static/js/* && rm ./zendesk-app/assets/static/css/*",
    "package": "rm ./zendesk-app/assets/static/js/*.map && cd ./zendesk-app && zat package && cd ../",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject"
  }
}
